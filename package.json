  "bugs": {
    "url": "https://github.com/abrahamgmacias/examen-alsuper/issues"
  },
  "homepage": "https://github.com/abrahamgmacias/examen-alsuper#readme",
  "dependencies": {
    "chai": "^4.3.10",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mocha": "^10.2.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.3",
    "nodemon": "^3.0.1",
    "sequelize": "^6.34.0",
    "sinon": "^17.0.1"
  }
